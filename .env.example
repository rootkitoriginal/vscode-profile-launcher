# AI API Configuration
# Get your Gemini API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Get your OpenAI API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Default AI Settings
DEFAULT_AI_PROVIDER=gemini
DEFAULT_GEMINI_MODEL=gemini-1.5-flash
DEFAULT_OPENAI_MODEL=gpt-3.5-turbo

# Application Settings
APP_NAME=VS Code Profile Launcher
APP_VERSION=1.0.0
DEBUG_MODE=false

# VS Code Settings
VSCODE_COMMAND=code
VSCODE_INSIDERS_COMMAND=code-insiders

# Database Settings
DB_NAME=profiles.db

# VS Code Profile Settings
# Base directory where all profile directories will be created
PROFILES_BASE_DIR=/home/rootkit/profiles

# Example command pattern:
# code "/caminho/para/seu/projeto" --user-data-dir "/home/rootkit/profiles/<profile_slug>/data" --extensions-dir "/home/rootkit/profiles/<profile_slug>/extensions"